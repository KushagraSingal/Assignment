line = 'Python is a case sensitive language'
#a
print('Length of string is',len(line))
#b
print(line[::-1])
#c
new_line = line[12:26]
print(new_line)
#d
print(line.replace('a case sensitive' , 'an object oriented'))
#e
print('Index of "a" in original string is' , line.find('a'))
#f
print(line.replace(' ', ''))
